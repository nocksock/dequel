# exakt

some:/FOOBAR/

==>

QueryList(
  Query(Condition(Field, ":", Matcher(Regex))),
)

# with flags

some:/FOOBAR/i

==>

QueryList(
  Query(Condition(Field, ":", Matcher(Regex))),
)

# with flags and complex stuff

some:/^[a-z\s ]\w{},*+/i

==>

QueryList(
  Query(Condition(Field, ":", Matcher(Regex))),
)

# escaped slash

some:/file\/path/i

==>

QueryList(
  Query(Condition(Field, ":", Matcher(Regex))),
)

# escaped slash slash

some:/file\\\/path/i

==>

QueryList(
  Query(Condition(Field, ":", Matcher(Regex))),
)
